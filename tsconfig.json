{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/component/Itemlist.jsx",
    "src/component/Item.jsx",
    "src/component/Header.jsx",
    "src/component/Gnb.jsx",
    "src/component/Footer.jsx",
    "pages/api/hello.js",
    "pages/index.jsx",
    "pages/about.jsx",
    "pages/_document.jsx",
    "pages/_app.jsx",
    "pages/view/[id].jsx"
  ],
  "exclude": ["node_modules"]
}
